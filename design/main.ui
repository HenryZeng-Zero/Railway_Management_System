                                    <property name="icon-name">list-remove-symbolic</property>
                                    <property name="label">删除数据</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkColumnView" id="page_1_col_view">
                            <property name="margin-end">10</property>
                            <property name="margin-start">10</property>
                            <property name="model">
                              <object class="GtkSingleSelection">
                                <property name="model">
                                  <object class="GtkSortListModel">
                                    <property name="model">
                                      <object class="GListStore" id="page_1_col_view_dataflow"/>
                                    </property>
                                    <property name="sorter">
                                      <object class="GtkColumnViewSorter"/>
                                    </property>
                                  </object>
                                </property>
                              </object>
                            </property>
                            <property name="show-column-separators">True</property>
                            <property name="show-row-separators">True</property>
                            <child>
                              <object class="GtkColumnViewColumn">
                                <property name="factory">
                                  <object class="GtkSignalListItemFactory"/>
                                </property>
                                <property name="title">列车名称</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkColumnViewColumn">
                                <property name="factory">
                                  <object class="GtkSignalListItemFactory"/>
                                </property>
                                <property name="title">出发地点</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkColumnViewColumn">
                                <property name="factory">
                                  <object class="GtkSignalListItemFactory"/>
                                </property>
                                <property name="title">到达地点</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkColumnViewColumn">
                                <property name="factory">
                                  <object class="GtkSignalListItemFactory"/>
